<% USING: continuations namespaces io kernel math math.parser
furnace webapps.pastebin calendar ; %>

<tr>
    <td>
        <a href="<% model get paste-link write %>">
            <% "summary" get write %>
        </a>
    </td>
    <td><% "author" get write %></td>
    <td><% "date" get timestamp>string print %></td>
</tr>
